CREATE TABLE Shopping (
    Item_ID NUMBER PRIMARY KEY,
    Item_Name VARCHAR2(50),
    Quantity NUMBER,
    Price NUMBER(10,2)
);

INSERT INTO Shopping (Item_ID, Item_Name, Quantity, Price) VALUES
(1, 'Shirt', 2, 1500.00),
 (2, 'Shoes', 1, 3500.00),
(3, 'Bag', 1, 2200.00),
 (4, 'Watch', 1, 5000.00);
